<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="0.0" android:centerColor="#ffffffff" />
    		<stroke android:width="1.0dip" android:color="@color/gray_ddd" />
    		<corners android:topLeftRadius="2dp" android:topRightRadius="2dp" android:bottomLeftRadius="2dp" android:bottomRightRadius="2dp" />
       
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
        
	        <gradient android:startColor="#ff9c9c9c" android:endColor="#ff9c9c9c" android:angle="0.0" android:centerColor="#ff9c9c9c" />
	    	<stroke android:width="1.0dip" android:color="@color/gray_ddd" />
	    	<corners android:topLeftRadius="2dp" android:topRightRadius="2dp" android:bottomLeftRadius="2dp" android:bottomRightRadius="2dp" />
        </shape>
    </item>
</selector>
